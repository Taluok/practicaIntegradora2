<!DOCTYPE html>
<html>
<head>
    <title>Listado de Productos</title>
</head>
<body>
    <h1>Listado de Productos</h1>
    <ul>
        {{#each products}}
            <li>
                <h2>{{title}}</h2>
                <p>{{description}}</p>
                <p>Category: {{this.category}}</p>
                <p>Precio: ${{price}}</p>
                <img src={{this.thumbnail}} alt={{this.title}}>
                <p>Code: {{this.code}}</p>
                <p>Stock: {{this.stock}}</p>
                <p>Status: {{this.status}}</p>
                <p>Id: {{this.id}}</p>
            </li>
        {{/each}}
    </ul>
</body>
</html>